    "azure-devops-primary": {
      "command": "node",
      "args": [".mcp-servers/azure-devops-mcp/dist/index.js", "msvante", "--authentication", "env"],
      "env": {
        "AZURE_DEVOPS_PAT": "YOUR_PRIMARY_ORG_PAT_HERE"
      }
    },
    "azure-devops-secondary": {
      "command": "node",
      "args": [".mcp-servers/azure-devops-mcp/dist/index.js", "another-org", "--authentication", "env"],
      "env": {
        "AZURE_DEVOPS_PAT": "YOUR_SECONDARY_ORG_PAT_HERE"
      }
    },
    "azure-devops-third": {
      "command": "node",
      "args": [".mcp-servers/azure-devops-mcp/dist/index.js", "third-org", "--authentication", "env"],
      "env": {
        "AZURE_DEVOPS_PAT": "YOUR_THIRD_ORG_PAT_HERE"
      }
    }
  }
}
